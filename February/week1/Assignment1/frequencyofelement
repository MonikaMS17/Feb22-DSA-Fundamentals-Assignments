class frequencyofelement
{
    
    static int countOccurrences(int arr[], int n, int key)
    {
        int res = 0;
        for (int i=0; i<n; i++)
            if (key == arr[i])
              res++;
        return res;
    }
     
    public static void main(String args[])
    {
        int arr[] = {1, 1, 2, 2, 4, 5, 5, 6, 6, 6, 6 ,9 ,10, 11 };
        int n = arr.length;
        int key = 6;
        System.out.println(countOccurrences(arr, n, key));
    }
}
